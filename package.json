{
  "name": "hyperdom",
  "version": "0.2.0",
  "description": "A fast, feature rich and simple framework for building dynamic browser applications.",
  "main": "index.js",
  "dependencies": {
    "vdom-parser": "1.3.4",
    "vdom-to-html": "2.3.1",
    "virtual-dom": "2.1.1"
  },
  "devDependencies": {
    "babel-preset-hyperdom": "^1.0.0",
    "benchmark": "2.1.0",
    "bluebird": "3.3.4",
    "browser-monkey": "2.4.1",
    "browserify": "14.1.0",
    "chai": "3.5.0",
    "httpism": "^2.7.0",
    "jquery": "2.1.3",
    "jquery-sendkeys": "4.0.0",
    "jsdom": "8.2.0",
    "karma": "1.5.0",
    "karma-browserify": "5.1.1",
    "karma-browserstack-launcher": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-ievms": "0.1.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.2",
    "karma-safari-launcher": "1.0.0",
    "lowscore": "1.12.1",
    "mocha": "3.2.0",
    "trytryagain": "1.2.0",
    "uglify-js": "2.4.16",
    "vdom-to-html": "2.0.0",
    "watchify": "3.9.0"
  },
  "scripts": {
    "test": "npm run karma && npm run mocha",
    "test-all": "BROWSERS=all npm test",
    "karma": "karma start --single-run",
    "mocha": "mocha test/server/*Spec.js",
    "build": "browserify browser.js > hyperdom.js && uglifyjs --compress warnings=false --mangle -- hyperdom.js > hyperdom.min.js",
    "prepublish": "npm run build",
    "size": "npm run build && gzip < hyperdom.min.js > hyperdom.min.js.gz && ls -lh hyperdom.*"
  },
  "keywords": [
    "virtual-dom",
    "front-end",
    "mvc",
    "framework",
    "html",
    "plastiq",
    "hyperdom"
  ],
  "author": "Tim Macfarlane <timmacfarlane@gmail.com>",
  "license": "MIT",
  "files": [
    "*.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/featurist/hyperdom.git"
  },
  "bugs": {
    "url": "https://github.com/featurist/hyperdom/issues"
  },
  "homepage": "https://github.com/featurist/hyperdom"
}
